{
  "name": "oleinpress-001",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.com/Olein-jp/oleinpress-001.svg?branch=master)](https://travis-ci.com/Olein-jp/oleinpress-001)",
  "main": "index.php",
  "scripts": {
    "cleanup:css": "rimraf assets/css && mkdir -p assets/css",
    "cleanup:images": "rimraf assets/images && mkdir -p assets/images",
    "cleanup:js": "rimraf assets/js && mkdir -p assets/js",
    "cleanup:packages": "rimraf assets/packages && mkdir -p assets/packages",
    "cleanup:zip": "rimraf oleinpress-001.zip",
    "css:style:sass": "node-sass --source-map true --include-path=node_modules/node-normalize-scss src/assets/sass/style.scss -o assets/css",
    "css:style:autoprefixer": "postcss assets/css/style.css --use autoprefixer -o assets/css/style.css",
    "css:style:postcss": "postcss assets/css/style.css -o assets/css/style.min.css -c=postcss.config.js",
    "css:style": "npm-run-all -s css:style:*",
    "css:editor-style:sass": "node-sass --include-path=node_modules/node-normalize-scss src/assets/sass/editor-style.scss -o .",
    "css:editor-style": "npm-run-all -s css:editor-style:*",
    "css": "npm run cleanup:css && npm-run-all -p css:*",
    "js:app:rollup": "rollup src/assets/js/script.js --dir assets/js -c=rollup.config.js",
    "js:app:uglifyjs": "uglifyjs assets/js/script.js -o assets/js/script.min.js -mc",
    "js:app": "npm-run-all -s js:app:*",
    "js": "npm run cleanup:js && npm-run-all -p js:*",
    "images": "npm run cleanup:images && cpy 'src/assets/images/**' assets/images",
    "packages:humburgers": "cpx 'node_modules/hamburgers/**' 'src/assets/packages/hamburgers'",
    "packages:superfish": "cpx 'node_modules/superfish/dist/**' 'src/assets/packages/superfish'",
    "packages:slick": "cpx 'node_modules/slick-carousel/slick/**' 'src/assets/packages/slick'",
    "packages:fontawesome": "cpx 'node_modules/@fortawesome/fontawesome-free/**' 'src/assets/packages/fontawesome'",
    "packages:move": "cpx 'src/assets/packages/**' 'assets/packages'",
    "packages": "npm run cleanup:packages && npm-run-all -p packages:*",
    "watch:css": "watch 'npm run css' ./src/assets/sass",
    "watch:js": "watch 'npm run js' ./src/assets/js",
    "watch:image": "onchange 'src/assets/images' -e '**/*.DS_Store' -- npm run images",
    "watch:php": "browser-sync start -p 'oleinpress001.wordpress' -f '**/*' 'assets/**'",
    "watch": "npm-run-all -p watch:*",
    "zip:pack": "composer install --no-dev && rsync -a --exclude='.*' --exclude='/node_modules' --exclude='*.config.js' --exclude='/src' . oleinpress-001",
    "zip:archive": "zip -9 -qmr oleinpress-001.zip oleinpress-001",
    "zip": "npm run cleanup:zip && npm run zip:pack && npm run zip:archive",
    "build": "npm run packages && npm-run-all -p js css images"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Olein-jp/oleinpress-001.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Olein-jp/oleinpress-001/issues"
  },
  "homepage": "https://github.com/Olein-jp/oleinpress-001#readme",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "autoprefixer": "^9.6.1",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.7",
    "cpx": "^1.5.0",
    "cpy-cli": "^2.0.0",
    "cssnano": "^4.1.10",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.0.0",
    "postcss-cli": "^6.1.3",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-js": "^3.6.0",
    "watch": "^1.0.2",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "hamburgers": "^1.1.3",
    "slick-carousel": "^1.8.1",
    "superfish": "^1.7.10"
  }
}
